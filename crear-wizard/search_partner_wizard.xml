<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wz_search_partner" model="ir.ui.view">
        <field name="name">wz_search_partner.form.view</field>
        <field name="model">wz.search.partner</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group col="4">
                        <field name="name" colspan="2"/>
                    </group>
                </sheet>
                <footer>
                    <button class="btn-primary" string="Guardar"  name="event_wizard" type="object"/>
                    <button class="btn-primary" string="Cancelar" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>
</odoo>